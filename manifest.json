{
  "manifest_version": 2,
  "name": "Tab Mover",
  "icons": {
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "description": "Separates all tabs matching a query into their own window.",
  "version": "0.1.1",
  "permissions": ["tabs"],
  "browser_action": {
    "default_title": "Tab Mover",
    "default_popup": "popup.html",
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    }
  }
}
